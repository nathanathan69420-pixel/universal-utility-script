local a=loadstring(game:HttpGet('https://sirius.menu/rayfield'))()local b=a:CreateWindow({Name="Utility Script",LoadingTitle="Educational Purposes Only ;)",LoadingSubtitle="Made with LOVE"})local c=b:CreateTab("Movement",4483362458)local d=game.Players.LocalPlayer local e,f,g,h,i,j,k,l,m,n,o,p,q,r,s=false,16,nil,false,nil,false,nil,nil,nil,50,false,nil,false,nil,Color3.fromRGB(255,255,255)local t=game:GetService("Lighting")local u={GlobalShadows=t.GlobalShadows,Brightness=t.Brightness,Ambient=t.Ambient,OutdoorAmbient=t.OutdoorAmbient,FogEnd=t.FogEnd,FogStart=t.FogStart,FogColor=t.FogColor,ClockTime=t.ClockTime,GeographicLatitude=t.GeographicLatitude,ExposureCompensation=t.ExposureCompensation}local v=t.FogEnd<math.huge local function w()if d.Character then local x=d.Character:FindFirstChildOfClass("Humanoid")if x then x.WalkSpeed=f end end end local function y()if d.Character then for z,A in pairs(d.Character:GetDescendants())do if A:IsA("BasePart")then A.CanCollide=false end end end end local function B()if d.Character and d.Character:FindFirstChild("HumanoidRootPart")then local C=d.Character.HumanoidRootPart if not k then k=Instance.new("BodyVelocity")k.MaxForce=Vector3.new(9e9,9e9,9e9)k.Velocity=Vector3.new(0,0,0)k.Parent=C end if not l then l=Instance.new("BodyGyro")l.MaxTorque=Vector3.new(9e9,9e9,9e9)l.P=1e4 l.Parent=C end local D=workspace.CurrentCamera l.CFrame=D.CFrame local E=game:GetService("UserInputService")local F=Vector3.new(0,0,0)if E:IsKeyDown(Enum.KeyCode.W)then F=F+D.CFrame.LookVector end if E:IsKeyDown(Enum.KeyCode.S)then F=F-D.CFrame.LookVector end if E:IsKeyDown(Enum.KeyCode.A)then F=F-D.CFrame.RightVector end if E:IsKeyDown(Enum.KeyCode.D)then F=F+D.CFrame.RightVector end if E:IsKeyDown(Enum.KeyCode.Space)then F=F+Vector3.new(0,1,0)end if E:IsKeyDown(Enum.KeyCode.LeftShift)then F=F-Vector3.new(0,1,0)end if F.Magnitude>0 then k.Velocity=F.Unit*n else k.Velocity=Vector3.new(0,0,0)end end end local function G()t.GlobalShadows=false t.Brightness=2 t.Ambient=Color3.fromRGB(255,255,255)t.OutdoorAmbient=Color3.fromRGB(255,255,255)t.FogEnd=1e6 t.FogStart=0 t.ClockTime=14 t.GeographicLatitude=41.733 t.ExposureCompensation=0 t.FogColor=Color3.fromRGB(255,255,255)if t:FindFirstChild("Atmosphere")then t.Atmosphere:Destroy()end end local function H()t.GlobalShadows=u.GlobalShadows t.Brightness=u.Brightness t.Ambient=u.Ambient t.OutdoorAmbient=u.OutdoorAmbient t.FogEnd=u.FogEnd t.FogStart=u.FogStart t.FogColor=u.FogColor t.ClockTime=u.ClockTime t.GeographicLatitude=u.GeographicLatitude t.ExposureCompensation=u.ExposureCompensation if not v then t.FogEnd=0 end end local function I()for J,K in pairs(game.Players:GetPlayers())do if K~=d and K.Character then local L=K.Character local M=L:FindFirstChild("ESPHighlight")or Instance.new("Highlight")if not M.Parent then M.Name="ESPHighlight"M.Parent=L end M.FillColor=s M.OutlineColor=s M.OutlineTransparency=0 M.FillTransparency=0.75 M.OutlineThickness=3 M.Enabled=true end end end local function N()for O,K in pairs(game.Players:GetPlayers())do if K.Character then local M=K.Character:FindFirstChild("ESPHighlight")if M then M:Destroy()end end end end local function P(Q)if e then w()end if h then y()end if j then k,l=nil,nil end if o then task.wait(0.5)I()end end d.CharacterAdded:Connect(P)c:CreateSlider({Name="Walkspeed",Range={16,200},Increment=1,Suffix=" Speed",CurrentValue=16,Callback=function(R)f=R if e then w()end end})c:CreateToggle({Name="Walkspeed",CurrentValue=false,Callback=function(R)e=R if e then g=game:GetService("RunService").Heartbeat:Connect(w)w()else if g then g:Disconnect()g=nil end if d.Character and d.Character:FindFirstChildOfClass("Humanoid")then d.Character:FindFirstChildOfClass("Humanoid").WalkSpeed=16 end end end})c:CreateToggle({Name="Noclip",CurrentValue=false,Callback=function(R)h=R if h then i=game:GetService("RunService").Heartbeat:Connect(y)y()else if i then i:Disconnect()i=nil end if d.Character then for z,A in pairs(d.Character:GetDescendants())do if A:IsA("BasePart")then A.CanCollide=true end end end end end})c:CreateSlider({Name="Fly Speed",Range={16,200},Increment=1,Suffix=" Speed",CurrentValue=50,Callback=function(R)n=R end})c:CreateToggle({Name="Fly",CurrentValue=false,Callback=function(R)j=R if j then m=game:GetService("RunService").Heartbeat:Connect(B)B()else if m then m:Disconnect()m=nil end if k then k:Destroy()k=nil end if l then l:Destroy()l=nil end end end})local S=b:CreateTab("Visuals",4483362458)S:CreateToggle({Name="Fullbright",CurrentValue=false,Callback=function(R)p=R if p then q=game:GetService("RunService").Heartbeat:Connect(G)G()else if q then q:Disconnect()q=nil end H()end end})S:CreateToggle({Name="Player ESP",CurrentValue=false,Callback=function(R)o=R if o then r=game:GetService("RunService").Heartbeat:Connect(I)I()else if r then r:Disconnect()r=nil end N()end end})S:CreateColorPicker({Name="ESP Color",Color=s,Callback=function(R)s=R if o then I()end end})
